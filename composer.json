{
  "name": "locrianphp/locrian",
  "type": "library",
  "description": "Locrian Framework Libraries",
  "keywords": ["php","framework","locrian","locrianframework"],
  "homepage": "https://github.com/locrianphp/locrian-libs",
  "license": "MIT",
  "authors": [
    {
      "name": "Özgür Senekci",
      "email": "ozgursenekci@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Locrian\\": "src/Locrian/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Locrian\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}